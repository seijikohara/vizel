{
  "name": "@vizel/tailwind",
  "version": "0.1.0",
  "description": "Tailwind CSS plugin for Vizel editor",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "vizel",
    "tailwindcss",
    "plugin",
    "editor",
    "rich-text"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seijikohara/vizel.git",
    "directory": "packages/tailwind"
  },
  "homepage": "https://github.com/seijikohara/vizel#readme"
}
