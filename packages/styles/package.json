{
  "name": "@vizel/styles",
  "version": "0.1.0",
  "description": "Pre-built CSS stylesheet for Vizel editor",
  "type": "module",
  "main": "./dist/vizel.css",
  "style": "./dist/vizel.css",
  "exports": {
    ".": "./dist/vizel.css",
    "./vizel.css": "./dist/vizel.css",
    "./vizel.min.css": "./dist/vizel.min.css",
    "./variables": "./dist/variables.css",
    "./shadcn": "./dist/variables-shadcn.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:css && bun run build:min",
    "build:css": "postcss src/index.css -o dist/vizel.css && cp src/variables.css dist/variables.css && cp src/variables-shadcn.css dist/variables-shadcn.css",
    "build:min": "postcss src/index.css -o dist/vizel.min.css --env production"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0"
  },
  "keywords": [
    "vizel",
    "css",
    "styles",
    "editor",
    "rich-text"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seijikohara/vizel.git",
    "directory": "packages/styles"
  },
  "homepage": "https://github.com/seijikohara/vizel#readme"
}
