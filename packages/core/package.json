{
  "name": "@vizel/core",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/seijikohara/vizel",
    "directory": "packages/core"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./styles.css": "./src/styles/index.scss",
    "./components.css": "./src/styles/components.scss"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./styles.css": "./dist/styles.css",
      "./components.css": "./dist/components.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm run build:css",
    "build:css": "npm run build:css:styles && npm run build:css:components",
    "build:css:styles": "sass --load-path=src/styles --load-path=node_modules src/styles/index.scss dist/styles.css --style=compressed",
    "build:css:components": "sass --load-path=src/styles --load-path=node_modules src/styles/components.scss dist/components.css --style=compressed",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "^1.18.0",
    "@iconify-json/lucide": "^1.2.84",
    "@iconify/utils": "^3.1.0",
    "@tiptap/core": "^3.15.3",
    "@tiptap/extension-blockquote": "^3.15.3",
    "@tiptap/extension-bold": "^3.15.3",
    "@tiptap/extension-bubble-menu": "^3.15.3",
    "@tiptap/extension-bullet-list": "^3.15.3",
    "@tiptap/extension-character-count": "^3.15.3",
    "@tiptap/extension-code": "^3.15.3",
    "@tiptap/extension-code-block": "^3.15.3",
    "@tiptap/extension-code-block-lowlight": "^3.15.3",
    "@tiptap/extension-color": "^3.15.3",
    "@tiptap/extension-details": "^3.15.3",
    "@tiptap/extension-document": "^3.15.3",
    "@tiptap/extension-drag-handle": "^3.15.3",
    "@tiptap/extension-dropcursor": "^3.15.3",
    "@tiptap/extension-file-handler": "^3.15.3",
    "@tiptap/extension-gapcursor": "^3.15.3",
    "@tiptap/extension-hard-break": "^3.15.3",
    "@tiptap/extension-heading": "^3.15.3",
    "@tiptap/extension-highlight": "^3.15.3",
    "@tiptap/extension-history": "^3.15.3",
    "@tiptap/extension-horizontal-rule": "^3.15.3",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-italic": "^3.15.3",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-list-item": "^3.15.3",
    "@tiptap/extension-list-keymap": "^3.15.3",
    "@tiptap/extension-ordered-list": "^3.15.3",
    "@tiptap/extension-paragraph": "^3.15.3",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/extension-strike": "^3.15.3",
    "@tiptap/extension-table": "^3.15.3",
    "@tiptap/extension-task-item": "^3.15.3",
    "@tiptap/extension-task-list": "^3.15.3",
    "@tiptap/extension-text": "^3.15.3",
    "@tiptap/extension-text-style": "^3.15.3",
    "@tiptap/extension-underline": "^3.15.3",
    "@tiptap/markdown": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/suggestion": "^3.15.3",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.27",
    "lowlight": "^3.3.0",
    "mermaid": "^11.12.2"
  },
  "devDependencies": {
    "vite": "^7",
    "vite-plugin-dts": "^4"
  }
}
